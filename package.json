{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js --ignore tests",
    "client": "open http://localhost:3000/ && npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "npm run build",
    "cache:clean": "node services/redisClean.js",
    "lint": "eslint --ext .js .",
    "lint:fix": "npm run lint -- --fix",
    "test": "bash scripts/launchTests.sh",
    "opensite": "open https://www.udemy.com/advanced-node-for-developers/learn/v4/content",
    "test:killall": "pgrep 'Chromium' | xargs kill -sigterm && echo 'Closed Chromium windows.'",
    "test:open": "echo 'open' > .TEST-STATUS && echo 'Leaving Chromium windows open.'",
    "test:close": "echo 'close' > .TEST-STATUS && echo 'Closing Chromium windows.'",
    "test:which": "echo 'Chromium windows status set to: '$(cat .TEST-STATUS)''"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.188.0",
    "babel-eslint": "^8.2.6",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-session": "^2.0.0-beta.2",
    "dotenv": "^6.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.15.3",
    "jest": "^22.1.4",
    "keygrip": "^1.0.2",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^4.11.1",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^2.0.2",
    "puppeteer": "^1.0.0",
    "redis": "^2.8.0",
    "safe-buffer": "^5.1.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "sinon": "^6.1.4"
  }
}
